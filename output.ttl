@prefix OWL: <http://example.org/OWL#> .
@prefix XSD: <http://example.org/XSD#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rico: <http://example.org/rico#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/denominazione_fascicolo_VALUE> a rico:Person ;
    rdfs:label "instance based on 'denominazione fascicolo'" ;
    OWL:sameAs "VIAF URI"^^xsd:string .

<http://example.org/denominazione_sottoserie_VALUE> a rico:Title ;
    rdfs:label "instance based on 'denominazione sottoserie'" .

<http://example.org/id_busta_VALUE> a rico:RecordSet ;
    rdfs:label "instance based on 'id_busta'" ;
    rico:generalDescription XSD:Literal ;
    rico:hasOrHadAppellation XSD:Literal ;
    rico:hasOrHadIdentifier rico:Identifier ;
    rico:identifier XSD:Literal ;
    rico:isDirectlyIncludedIn rico:RecordSet .

<http://example.org/id_documento_VALUE> a rico:Record ;
    rdfs:label "instance based on 'id_documento'" ;
    rico:dateOrDateRange rico:date ;
    rico:generalDescription XSD:Literal ;
    rico:hasBeginningDate rico:date ;
    rico:hasCreationDate rico:date ;
    rico:hasEndDate rico:date ;
    rico:hasOrHadAppellation XSD:Literal ;
    rico:hasOrHadIdentifier rico:Identifier ;
    rico:hasSender "Persona URI"^^xsd:string ;
    rico:isAssociatedWithPlace rico:place ;
    rico:physicalOrLogicalExtent rdfs:Literal .

<http://example.org/id_fascicolo_VALUE> a rico:RecordSet ;
    rdfs:label "instance based on 'id_fascicolo'" ;
    rico:dateOrDateRange rico:date ;
    rico:hasBeginningDate rico:date ;
    rico:hasCreationDate rico:date ;
    rico:hasEndDate rico:date ;
    rico:hasOrHadAppellation XSD:Literal ;
    rico:hasOrHadIdentifier rico:Identifier ;
    rico:hasSender "Persona URI"^^xsd:string ;
    rico:physicalOrLogicalExtent XSD:Literal .

<http://example.org/id_immagine_VALUE> a rico:RecordResource ;
    rdfs:label "instance based on 'id_immagine'" ;
    rico:hasOrHadAppellation XSD:Literal ;
    rico:hasOrHadIdentifier rico:Identifier .

<http://example.org/id_serie_VALUE> a rico:RecordSet ;
    rdfs:label "instance based on 'id_serie'" ;
    rico:dateOrDateRange rico:date ;
    rico:directlyIncludes rico:RecordSet ;
    rico:generalDescription XSD:Literal ;
    rico:hasBeginningDate rico:date ;
    rico:hasEndDate rico:date ;
    rico:hasOrHadAppellation XSD:Literal ;
    rico:hasOrHadIdentifier rico:Identifier ;
    rico:physicalOrLogicalExtent XSD:Literal .

<http://example.org/id_sottoserie_VALUE> a rico:RecordSet ;
    rdfs:label "instance based on 'id_sottoserie'" ;
    rico:dateOrDateRange rico:date ;
    rico:directlyIncludes rico:RecordSet ;
    rico:generalDescription XSD:Literal ;
    rico:hasBeginningDate rico:date ;
    rico:hasEndDate rico:date ;
    rico:hasOrHadIdentifier rico:Identifier ;
    rico:hasOrHadTitle rico:Title ;
    rico:identifier XSD:Literal ;
    rico:isDirectlyIncludedIn rico:RecordSet ;
    rico:physicalOrLogicalExtent XSD:Literal .

<http://example.org/mittenti_extra_VALUE> a rico:Person ;
    rdfs:label "instance based on 'mittenti extra'" ;
    OWL:sameAs "VIAF extra"^^xsd:string .

