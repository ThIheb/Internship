@prefix OWL: <http://example.org/OWL#> .
@prefix XSD: <http://example.org/XSD#> .
@prefix ex: <http://example.org/> .
@prefix place: <http://example.org/place/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rico: <http://www.ica.org/standards/RiC/ontology#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:Beginning_date_URI a rico:Date ;
    rdf:value xsd:Literal .

ex:Busta_URI a rico:RecordSet ;
    rico:Identifier XSD:Literal ;
    rico:generalDescription XSD:Literal ;
    rico:hasOrHadAppellation XSD:Literal ;
    rico:identifier XSD:Literal ;
    rico:isDirectlyIncludedIn ex:ricoRecordSet .

ex:Denominazione_Fascicolo OWL:sameAs "VIAF URI"^^xsd:string .

ex:Documento_URI rico:Identifier XSD:Literal ;
    rico:generalDescription XSD:Literal ;
    rico:hasBeginningDate XSD:Integer ;
    rico:hasEndDate XSD:Integer ;
    rico:hasOrHadAppellation XSD:Literal ;
    rico:hasOrHadLocation place:ricoplace ;
    rico:hasSender <http://example.org/entity/Persona_URI> ;
    rico:normalizedDateValue XSD:Integer ;
    rico:physicalOrLogicalExtent rdfs:Literal .

ex:End_date_URI a rico:Date ;
    rdf:value XSD:Literal .

ex:Fascicolo_URI a rico:RecordSet ;
    rico:Identifier XSD:Literal ;
    rico:hasBeginningDate XSD:integer ;
    rico:hasEndDate XSD:integer ;
    rico:hasOrHadAppellation XSD:Literal ;
    rico:hasSender <http://example.org/entity/Persona_URI> ;
    rico:identifier XSD:Literal ;
    rico:normalizedDateValue XSD:Integer ;
    rico:physicalOrLogicalExtent XSD:Literal .

ex:Immagini_URI a rico:RecordResource ;
    rico:Identifier XSD:Literal ;
    rico:hasOrHadAppellation XSD:Literal .

ex:Persona_URI a rico:Person .

ex:Physical_Location_URI a rico:Place .

ex:Serie_URI a rico:RecordSet ;
    rico:Identifier XSD:Literal ;
    rico:directlyIncludes rico:RecordSet ;
    rico:generalDescription XSD:Literal ;
    rico:hasBeginningDate "Beginning date URI"^^xsd:string ;
    rico:hasEndDate "End date URI"^^xsd:string ;
    rico:hasOrHadAppellation XSD:Literal ;
    rico:identifier XSD:Literal ;
    rico:physicalOrLogicalExtent XSD:Literal .

ex:Sottoserie_URI a rico:RecordSet ;
    rico:Identifier XSD:Literal ;
    rico:directlyIncludes rico:RecordSet ;
    rico:generalDescription XSD:Literal ;
    rico:hasBeginningDate "Beginning date URI"^^xsd:string ;
    rico:hasEndDate "End date URI"^^xsd:string ;
    rico:hasOrHadTitle XSD:Literal ;
    rico:identifier XSD:Literal ;
    rico:isDirectlyIncludedIn ex:ricoRecordSet ;
    rico:physicalOrLogicalExtent XSD:Literal .

ex:mittenti_extra OWL:sameAs "VIAF extra"^^xsd:string .

place:ricoplace a rico:Place ;
    rdfs:label "rico:place" .

<http://example.org/entity/Persona_URI> a rico:Agent ;
    rdfs:label "Persona URI" .

